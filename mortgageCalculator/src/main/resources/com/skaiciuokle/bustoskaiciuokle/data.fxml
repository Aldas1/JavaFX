      </TableView>
      <ButtonBar layoutX="14.0" layoutY="346.0" prefHeight="40.0" prefWidth="185.0">
        <buttons>
            <Button fx:id="chart" mnemonicParsing="false" text="Line chart" />
            <Button fx:id="export" mnemonicParsing="false" text="Export" />
        </buttons>
      </ButtonBar>
